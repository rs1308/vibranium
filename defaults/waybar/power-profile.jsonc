{
	"custom/power-profile": {
		"format": "{text}",
		"hide-empty-text": true,

		"on-scroll-up": "vibranium-core-power-profile-set up",
		"on-scroll-down": "vibranium-core-power-profile-set down",

		"on-click": "vibranium-core-power-profile-set power-saver",
		"on-click-middle": "vibranium-core-power-profile-set balanced",
		"on-click-right": "vibranium-core-power-profile-set performance",

		"exec": "vibranium-bar-power-profile",
		"signal": 7,
		"return-type": "json"
	}
}
